# PostgreSQL
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=intrasite

# MinIO
MINIO_ROOT_USER=admin
MINIO_ROOT_PASSWORD=admin123

# Next.js and Auth
# Replace with your actual database URL
# Example: postgresql://user:password@localhost:5432/dbname
# Or Prisma Accelerate: prisma+postgres://accelerate.prisma-data.net/?api_key=YOUR_API_KEY
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/intrasite"

# NextAuth Configuration

# Generate with: openssl rand -base64 32
AUTH_SECRET=your-secret-key-here

# Docker Compose
COMPOSE_PROJECT_NAME=intrasite

# Environment
NODE_ENV=development

# Google OAuth 2.0 Client ID (Public Identifier)
GOOGLE_CLIENT_ID=your-client-id-here

# Google OAuth 2.0 Client Secret
GOOGLE_CLIENT_SECRET=your-secret-key-here

# GitHub OAuth App Client ID (Public Identifier)
GITHUB_CLIENT_ID=your-client-id-here

# GitHub OAuth App Client Secret
GITHUB_CLIENT_SECRET=your-secret-key-here

# Resend
RESEND_API_KEY=your-secret-key-here